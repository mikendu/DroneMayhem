#:kivy 1.11.1
#:import utils kivy.utils

<Dashboard>:
    menu: fileMenu

    canvas:
        Color: 
            rgb: utils.get_color_from_hex('#292929')
        Rectangle:
            pos: 0, 0
            size: root.width, root.height

    FileMenu:
        id: fileMenu
        pos: 0, root.top - 30
        size: root.width, 30

    
